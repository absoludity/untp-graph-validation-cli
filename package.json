{
  "name": "untp-graph-validation-cli",
  "version": "0.1.0",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "untp-validator": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc && cp -r src/core/queries dist/core/",
    "start": "node dist/index.js",
    "dev": "ts-node --esm src/index.ts",
    "validate": "node --no-warnings dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "query": "eyereasoner --nope --quiet --pass-only-new"
  },
  "keywords": [
    "untp",
    "validation",
    "cli",
    "credentials"
  ],
  "author": "",
  "license": "GPL-3.0-only",
  "description": "Library and CLI tool to validate UNTP credential files",
  "dependencies": {
    "@comunica/query-sparql": "^4.1.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "eyereasoner": "^18.14.4",
    "jsonld": "^8.3.3",
    "n3": "^1.24.2"
  },
  "devDependencies": {
    "@types/jsonld": "^1.5.15",
    "@types/n3": "^1.24.2",
    "@types/node": "^22.13.13",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  },
  "exports": {
    ".": "./dist/index.js",
    "./cli": "./dist/cli/index.js",
    "./core": "./dist/core/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
